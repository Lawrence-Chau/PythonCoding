#___ITERATION OF LISTS___#
origami_types = ["crane", "star", "dragon"]
origami_types[1] = "cat"
#Iterating using RANGE:
for x in range(3):
  # print(x)
  # print(f'the value of num is {x}')
  # print("the value of num is" + str(x))
  print(origami_types[x])

#Iterating Directly:
for a_list in origami_types:
  print(f"i can make a {a_list}")
